@section Scripts{
    <script src="~/js/signalr.js"></script>
    <script>
        const randomColor = Math.floor(Math.random() * 16777215).toString(16);
        var flag = false;
        var chatcontain = document.getElementById("chatcontain");
        $("#chatbtn").on('click', () => {
            flag = !flag;
            if (flag) {
                chatcontain.style.display = "block";
            } else {
                chatcontain.style.display = "none";
            }

        })
        "use strict";  //嚴謹模式
        var connection = new signalR.HubConnectionBuilder().withUrl("/chathub").build();  //建立連接
        var username = "";

        //將傳送按鈕藏起 直到連線正常
        $("#sendButton").attr("disabled", true)

        connection.on("ReceiveMessage", function (user, message, time, color) {
            //TODO 修改邊框聊天圖案
            var msg = message.replace(/&/g, "&amp;").replace(/</g, "&lt;").replace(/>/g, "&gt;"); //& , < , > 設定三個符號轉譯
            var encodeMsg = msg;
            var t = document.createElement("p");
            t.innerHTML = time;
            t.style.fontSize = "12px";
            t.style.color = "black";
            t.style.display = "inline";
            var li = document.createElement("li");
            li.style.listStyleType = "none";
            li.style.fontSize = "15px";
            li.style.fontWeight = "600";
            li.style.marginTop = "10px";
            if (user == username) {
                li.style.color = "black";
                li.style.textAlign = "right";
                li.append(t, " ", encodeMsg);
            } else {
                var userelse = document.createElement("p");
                userelse.innerHTML = user;
                userelse.style.position = "absolute";
                userelse.style.display = "inline";
                userelse.style.top = "-12px";
                userelse.style.fontSize = "13px";

                var Msg = document.createElement("p");
                Msg.innerHTML = encodeMsg;
                Msg.style.display = "inline";

                li.style.color = "#5A5AAD";
                li.style.textAlign = "left";
                li.style.position = "relative";
                li.append(userelse, Msg, " ", t);
            }
            document.getElementById("messagesList").appendChild(li);
            document.getElementById("chatmain").scrollTop = document.getElementById("chatmain").scrollHeight;
        })

        connection.on("NewJoin", function (user, message, time, color) {
            var t = document.createElement("p");
            t.innerHTML = time;
            t.style.fontSize = "16px";
            t.style.color = "black";
            t.style.display = "inline";

            var li = document.createElement("li");
            li.style.fontSize = "16px";
            li.style.color = "gray";
            li.innerHTML = `${t.innerHTML} ${user} ${message}`;
            li.style.textAlign = "center";
            document.getElementById("messagesList").appendChild(li);
            document.getElementById("chatmain").scrollTop = document.getElementById("chatmain").scrollHeight;
        })

        connection.on("Leave", function (user, message, time, color) {
            var t = document.createElement("p");
            t.innerHTML = time;
            t.style.fontSize = "16px";
            t.style.color = "gray";
            t.style.display = "inline";

            var li = document.createElement("li");
            li.style.fontSize = "16px";
            li.style.color = "gray";
            li.innerHTML = `${t.innerHTML} ${user} ${message}`;
            document.getElementById("messagesList").appendChild(li);
            document.getElementById("chatmain").scrollTop = document.getElementById("chatmain").scrollHeight;
        })

        connection.start().then(function () {
            document.getElementById("sendButton").disabled = false;  //連結建立成功 將傳送按鈕顯示
        }).catch(function (err) {
            return console.error(err.toString());
        })

        //$("#bye").on("click", () => {
        //    connection.onclose(connection.invoke("SendMessage", username, "離開聊天室", ""))
        //})

        $("#sendButton").on("click", function (event) {
            var message = $('#message').val();
            connection.invoke("SendMessage", username, message, "", randomColor).then(function () {
                document.getElementById('message').value = "";
            }).catch(function (err) {
                return console.error(err.toString());
            })
            event.preventDefault();  //取消DOM默認動作
        })

        function SetUsername() {
            //檢查使用者是否輸入暱稱
            var usernameinput = $('#username').val();
            if (usernameinput === "") {
                alert("務必輸入加入聊天室的暱稱")
                return;
            }
            username = usernameinput;

            document.getElementById('userinfo').style.display = 'none';
            document.getElementById('messagearea').style.display = 'block';
            document.getElementById("username1").innerText = usernameinput;

            connection.invoke("SendMessage", username, "加入聊天室", "", "");
        }

        function demo1() {
            document.getElementById("message").value = "大家好，第一次來，請問有什麼推薦的貓零食嗎";
        }
        function demo2() {
            document.getElementById("message").value = "快吃肉乾的忘憂神仙草還不錯，推薦給你!";
        }
        function demo3() {
            document.getElementById("message").value = "好的謝謝!我馬上去看看";
        }
    </script>






